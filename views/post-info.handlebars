<div class="existing-blog-posts">
  {{#each blogPosts as |blogPost|}}
  <div class="post">
    <h1 class="title">{{blogPost.title}}</h1>
    <p class="content">{{blogPost.content}}</p>
    <p class="username">{{blogPost.username}}</p>
    <p class="dateCreated">{{blogPost.dateCreated}}</p>
  </div>
  {{/each}}
</div>

{{!-- might need to add .js to the src path --}}
<script src="/js/comment-post"></script>